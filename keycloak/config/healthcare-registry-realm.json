    "realm": [
      {
        "name": "SUPER_ADMIN",
        "description": "Super administrator with full system access",
        "composite": true,
        "composites": {
          "realm": ["ADMIN"]
        },
        "attributes": {
          "permissions": ["FULL_SYSTEM_ACCESS", "EMERGENCY_ACCESS", "DATA_EXPORT"]
        }
      },
      {
        "name": "ADMIN",
        "description": "System administrator",
